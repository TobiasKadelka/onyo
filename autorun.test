#!/usr/bin/env bash

# create default folders to prepare the onyo-repo.
mkdir shelf/
mkdir user/

#onyo
onyo_init .

onyo_new shelf <<EOF
laptop
apple
macbookpro
867
EOF

onyo_new shelf <<EOF
laptop
apple
macbookpro
2345
EOF

onyo_new shelf <<EOF
laptop
apple
macbookpro
99999999
EOF

git add shelf
git add user

onyo_mv shelf/laptop_apple_macbookpro.2345 user/
onyo_mv --rename shelf/laptop_apple_macbookpro.99999999 user/laptop_apple_macbookpro.88888
onyo_mv --rename --force shelf/laptop_apple_macbookpro.867 user/laptop_apple_macbookpro.88888
